<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context=".activity.GoodList01Activity">
    <include layout="@layout/view_feed_toolbar"></include>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">

        <include layout="@layout/goods_list_view01"></include>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/dp_10"
                android:orientation="vertical">

                <likeit.com.jingdong.view.BorderRelativeLayout
                    android:id="@+id/rl_home"
                    android:layout_width="@dimen/width80"
                    android:layout_height="@dimen/width80"
                    android:layout_margin="@dimen/dp_10"
                    app:contentBackColor="#50000000"
                    app:cornerRadius="40dp"
                    app:strokeColor01="#50000000"
                    app:strokeWidth01="0dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:src="@mipmap/icon_home" />
                </likeit.com.jingdong.view.BorderRelativeLayout>

                <likeit.com.jingdong.view.BorderRelativeLayout
                    android:id="@+id/rl_back"
                    android:layout_width="@dimen/width80"
                    android:layout_height="@dimen/width80"
                    android:layout_margin="@dimen/dp_10"
                    app:contentBackColor="#50000000"
                    app:cornerRadius="40dp"
                    app:strokeColor01="#50000000"
                    app:strokeWidth01="0dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:src="@mipmap/icon_back_1" />
                </likeit.com.jingdong.view.BorderRelativeLayout>

                <likeit.com.jingdong.view.BorderRelativeLayout
                    android:id="@+id/rl_top"
                    android:layout_width="@dimen/width80"
                    android:layout_height="@dimen/width80"
                    android:layout_margin="@dimen/dp_10"
                    app:contentBackColor="#50000000"
                    app:cornerRadius="40dp"
                    app:strokeColor01="#50000000"
                    app:strokeWidth01="0dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:src="@mipmap/icon_top" />
                </likeit.com.jingdong.view.BorderRelativeLayout>
            </LinearLayout>
        </RelativeLayout>

    </FrameLayout>
</LinearLayout>